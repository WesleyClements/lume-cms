{
  "lock": false,
  "tasks": {
    "dev": "deno run --unstable-kv --check --watch -A dev.ts",
    "update-deps": "deno run -A --quiet 'https://deno.land/x/nudd@v0.1.2/cli.ts' deps/*.ts",
    "build-libs": "cd scripts && npm run build"
  },
  "exclude": [
    "probas",
    "static/libs",
    "CHANGELOG.md",
    "node_modules",
    "dist"
  ]
}
